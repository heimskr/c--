#name "Static Methods"
#author "Kai Tamkun"
#orcid "0000-0001-7405-6654"
#version "1.0"

struct Foo {
	u8 ch;
	void print();
	static void create(%Foo* out);
	static u8 const *str = "Hello!";
	static s64 empty;
};

static void Foo::create(%Foo* out) {
	out->ch = '*';
}

void Foo::print() {
	`c(this->ch);
	`c('\n');
	`u64(sizeof(%Foo));
	`c('\n');
}

void main() {
	%Foo foo;
	%Foo::create(&foo);
	foo::print();
}
